(chart/linechart
    data-x (csv "tests/testdata/measurement.csv" time)
    data-y (csv "tests/testdata/measurement.csv" value1)
    margin-right 3em
    axis-left (
        label-placement (subdivide 4)
        label-format (scientific)
        label-attach bottom
        label-rotate -90)
    axis-bottom (
        label-format (datetime "%H:%M:%S")
        label-attach left
        label-rotate 45))
