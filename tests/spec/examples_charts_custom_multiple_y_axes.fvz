(plot
    axis-left (
        limit (0 100)
        label-placement (linear-alignat 20 10))
    axis-right (
        limit (0 1)
        label-placement (linear 0.1))
    body (
      (plot/points
          data-x (100px 500px 700px 400px 300px)
          data-y (200px 300px 200px 380px 100px)
          data-color (#00c #c00)
          labels (A B A B A)
          label-font-size 18pt
          marker-size 8px)))
