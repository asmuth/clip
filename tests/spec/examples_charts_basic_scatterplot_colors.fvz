(set height 240px)
(set width 900px)

(chart/scatterplot
    data-x (csv tests/testdata/gauss3d.csv x)
    data-y (csv tests/testdata/gauss3d.csv y)
    data-color (csv tests/testdata/gauss3d.csv z)
    limit-x (0 400)
    limit-y (0 200)
    color-map (gradient (0 #DC526A) (1.0 #D6B207))
    axis-y-label-placement (subdivide 5)
    axes (bottom left))
